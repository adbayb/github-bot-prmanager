{
	"env": {
        "browser": true,
		"node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
		},
		"sourceType": "module"
	},
    "extends": [
        "eslint:recommended"
    ],
	"plugins": [
        "prettier"
    ],
	"rules": {
        "prettier/prettier": "error",
        "no-console": "off"
	}
}